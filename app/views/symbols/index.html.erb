<form action="/symbols" method="get">
  <input type="text" name="query" placeholder="Search...">
  <input type="submit" value="Search">
</form>

<ul>
  <% if @symbols.present? %>
    <% @symbols.each do |a| %>
      <li>
        <span><%= a["symbol"] %></span>
        <span><%= a["prevDayClosePrice"] %></span>
      </li>
    <% end %>
  <% end %>
</ul>